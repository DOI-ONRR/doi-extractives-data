{
  "private": true,
  "homepage": "https://useiti.doi.gov/",
  "author": "18F",
  "license": "CC0-1.0",
  "repository": "github:18F/doi-extractives-data",
  "bugs": {
    "url": "https://github.com/18F/doi-extractives-data/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prestart": "if [ ! -d node_modules ]; then npm install; fi",
    "build": "rm -rf _site; bundle exec jekyll build",
    "federalist": "webpack && npm run release-styleguide",
    "start-states": "bundle exec jekyll serve --incremental --config _config.yml,_maps.yml",
    "test": "mocha test/*.js test/**/*.js",
    "test-html": "bundle exec htmlproof --disable-external _site",
    "test-ruby": "bundle exec rubydoctest _plugins/eiti_*.rb",
    "webpack": "webpack",
    "update-webpack": "webpack --debug --devtool source-map --output-pathinfo",
    "js-dev-start": "rm -rf js/lib && npm run watch-webpack",
    "js-dev-end": "npm run update-webpack",
    "watch-webpack": "webpack --watch",
    "build-styleguide": "cd nrrd-design-system && fractal build && cd ..",
    "release-styleguide": "npm run build-styleguide",
    "watch-styleguide": "cd nrrd-design-system && fractal start --sync && cd ..",
    "lint": "(npm run lint-js; npm run lint-scss) || echo '(there were linting errors)'",
    "lint-js": "eslint --ignore-path .eslintignore js",
    "lint-ruby": "bundle exec rubocop -c .rubocop.yml _plugins/eiti_*.rb",
    "lint-scss": "bundle exec scss-lint -c .scss-lint.yml || echo '(there were SCSS linting errors)'",
    "sqlite3": "^3.1.9"
  },
  "dependencies": {
    "@frctl/fractal": "^1.1.7",
    "async": "^0.9.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "custom-event": "^1.0.0",
    "d3": "^3.5.5",
    "d3-queue": "^1.0.7",
    "d3-svg-legend": "^1.10.0",
    "document-register-element": "^0.5.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "immutable": "^3.7.6",
    "jquery": "^1.11.3",
    "list.js": "^1.1.1",
    "lunr": "^0.7.2",
    "node-sass": "^4.7.2",
    "object-assign": "^4.1.0",
    "pty.js": "github:chjj/pty.js",
    "sass-loader": "^6.0.6",
    "stickyfill-web-module": "^1.1.11",
    "svg4everybody": "github:18f/svg4everybody#non-symbol",
    "svgeo": "^0.3.5",
    "topojson": "github:mbostock/topojson#oversize",
    "webpack": "*"
  },
  "devDependencies": {
    "chokidar": "^2.0.0",
    "colors": "^1.1.0",
    "data-expression": "^0.3.2",
    "epipebomb": "^0.1.1",
    "eslint": "^3.5.0",
    "express": "^4.16.2",
    "extend": "^2.0.0",
    "glob": "^7.1.1",
    "js-yaml": "^3.3.1",
    "jsdom": "^8.4.0",
    "lodash": "^4.15.0",
    "mocha": "^2.3.3",
    "nestly": "^0.2.0",
    "request": "^2.55.0",
    "rw": "^0.1.4",
    "sequelize": "^3.20.0",
    "sqlite3": "^3.1.2",
    "stream-array": "^1.0.1",
    "svgo": "^0.6.6",
    "tables": "^0.0.4",
    "through2": "^0.6.5",
    "tito": "^0.6.1",
    "to-jekyll-collection": "^0.1.1",
    "yargs": "^3.7.1"
  }
}
